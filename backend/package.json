    "@types/jsonwebtoken": "^9.0.6",
    "@types/module-alias": "^2.0.4",
    "@types/newrelic": "^9.14.3",
    "@types/node": "^20.12.5",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.8",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "nock": "^13.5.4",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.4"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "express": "^4.19.2",
    "flatbuffers": "^24.3.25",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "http-graceful-shutdown": "^3.1.13",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "module-alias": "^2.2.3",
